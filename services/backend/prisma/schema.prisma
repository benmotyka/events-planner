generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

model Event {
    id          String @id @default(uuid())
    title       String
    description String

    latitude  Float
    longitude Float

    date      DateTime  @default(now())
    createdAt DateTime  @default(now())
    updatedAt DateTime  @updatedAt
    deletedAt DateTime?

    @@map("event")
}
